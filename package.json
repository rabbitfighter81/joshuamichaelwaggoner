{
  "name": "joshuamichaelwaggoner",
  "version": "0.0.0-beta.274",
  "scripts": {
    "heroku-postbuild": "bash ./locale-postbuild.sh",
    "start": "node server/server.js",
    "config": "ts-node ./scripts/set-env.ts",
    "ng": "ng",
    "build": "ng build-locale:en",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle-report": "ng build --prod --stats-json && webpack-bundle-analyzer dist/stats.json",
    "serve-en": "npm run config -- --environment=dev && ng serve --aot --i18n-file=src/locale/messages.en.xlf --locale=en --i18n-format=xlf",
    "serve-es": "npm run config -- --environment=dev && ng serve --aot --i18n-file=src/locale/messages.es.xlf --locale=en --i18n-format=xlf",
    "serve-de": "npm run config -- --environment=dev && ng serve --aot --i18n-file=src/locale/messages.de.xlf --locale=en --i18n-format=xlf",
    "deploy": "git push && git push heroku master",
    "locale-postbuild": "bash ./locale-postbuild.sh",
    "build-locale:en": "npm run config -- --environment=prod && ng build --prod --aot --i18n-locale en --i18n-format xlf --i18n-file src/translate/messages.en.xlf --output-path=dist/en --baseHref /en/",
    "build-locale:de": "npm run config -- --environment=prod && ng build --prod --aot --i18n-locale de --i18n-format xlf --i18n-file src/translate/messages.de.xlf --output-path=dist/de --baseHref /de/",
    "build-locale:es": "nnpm run config -- --environment=prod && ng build --prod --aot --i18n-locale es --i18n-format xlf --i18n-file src/translate/messages.es.xlf --output-path=dist/es --baseHref /es/",
    "build-locale": "npm run build-locale:en && npm run build-locale:de & npm run build-locale:es"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.14",
    "@angular/cdk": "~7.3.7",
    "@angular/cli": "~7.3.8",
    "@angular/common": "~7.2.14",
    "@angular/compiler": "~7.2.14",
    "@angular/compiler-cli": "~7.2.14",
    "@angular/core": "~7.2.14",
    "@angular/forms": "~7.2.14",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "~7.2.14",
    "@angular/platform-browser-dynamic": "~7.2.14",
    "@angular/router": "~7.2.14",
    "@types/mocha": "^5.2.6",
    "@types/yargs": "^13.0.0",
    "compression": "^1.7.4",
    "core-js": "^2.5.4",
    "cors": "^2.8.5",
    "disconnect": "^1.2.1",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "fs": "0.0.1-security",
    "hammerjs": "^2.0.8",
    "http": "0.0.0",
    "lodash.assign": "^4.2.0",
    "lodash.findindex": "^4.6.0",
    "mongoose": "^5.5.5",
    "nodemailer": "^6.1.1",
    "path": "^0.12.7",
    "rxjs": "^6.5.1",
    "tslib": "^1.9.3",
    "yargs": "^13.2.2",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^11.13.10",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.18.11",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2",
    "webpack-bundle-analyzer": "^3.3.2"
  }
}
